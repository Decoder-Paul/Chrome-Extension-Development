{
    "manifest_version": 3,
    "name": "Timer",
    "version": "1.0.0",
    "description": "Learning of extionsion developemnt using Timer",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action":{
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        },
        "default_title": "Timer Extension Learning",
        "default_popup": "popup.html"
    },
    "options_page": "options_page.html",
    "permissions": ["storage", "alarms", "notifications"],
    "background": {
        "service_worker": "background.js"
    }
}